/* To access +, -, *, /, and = as simple calculations through a generic calculator there should be functions that call from math
and prepare for the next round of input. */
 
function add(valueIn, answer){
	answer = valueIn + answer;
 
	return answer;
 }
 
function subt(valueIn, answer){
	answer = answer - valueIn;
 
	return answer;
 }
 
 function mult(valueIn, answer){
	answer = valueIn * answer;
 
	return answer;
 }
 
 function divi(valueIn, answer){
	answer = answer / valueIn;
 
	return answer;
 }
  
 function equal(valueIn, answer){
 	alert(answer); // display answer. Only need to display because each operator already handles the arithmetic.
	answer = 0; // attempt to clear answer for new input.
 }
 
 
 
